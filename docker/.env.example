# FeedSpine Docker Environment
# Copy this to .env and customize

# Database passwords (change in production!)
POSTGRES_PASSWORD=feedspine

# Port mappings (change if conflicts)
POSTGRES_PORT=5432
TIMESCALE_PORT=5433
PGBOUNCER_PORT=6432
REDIS_PORT=6379
ADMINER_PORT=8080

# Data directory (where database files are stored)
# Default: ./data relative to docker-compose.yml
# Example: /var/lib/feedspine/data
FEEDSPINE_DATA_DIR=./data
